@{ var notas_comparti = ((List<Nota_Compartida>)ViewBag.Nota_Compartidas);                var notas_comparti_text = ((List<Nota_Compartida>)ViewBag.Nota_Compartidas_text);}<div class="text-center">    <h1 name="notas123" id="notas123" class="text-body">MIS NOTAS COMPARTIDAS</h1></div><br /><br /><div class="row" id="Lista"></div><!-- Modal --><div class="container" id="" tabindex="-1" aria-labelledby="" >    <div class="list-group">        <div class="container">            <div class="category_list">                <div class="container-fluid bg-dark">                    <ul class="list-unstyled">                        @foreach (var item in notas_comparti)                        {                    <li><label style="color:palegreen">User: </label> <a class="container-fluid text-warning" href="#">@item.user.Username</a></li>                                    @foreach (var item2 in notas_comparti_text)                                    {                    <li><label style="color:aqua">Titulo: </label> <a class="container-fluid text-warning" href="#">@item2.nota.Tittle</a></li>                    <li><label style="color:red">Contenido: </label> <a class="container-fluid text-warning" href="#">@item2.nota.Nota_Content</a></li>                    <label style="color:blueviolet">--------------------------------------------------------------------------------------------------------------------------</label>                                                            }}                    </ul>                </div>            </div>            </div>                    </div>    </div>@*@section Scripts{    <script>        $.ajax({            url: '/Notas/Create',            type: 'get'        }).done(function (html) {            $('#Cnota').html(html);        })        function submitFormCrear() {            $('#createNota').submit();        }        function guardar(event) {            event.preventDefault();  